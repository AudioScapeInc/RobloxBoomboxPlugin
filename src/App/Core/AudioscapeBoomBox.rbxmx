-- Sets up a new draggable object
function DraggableObject.new(Object, CallBack)
	local self = {}
	self.Object = Object
	self.DragStarted = nil
	self.DragEnded = nil
	self.Dragged = nil
	self.Dragging = false
	self.CallBack = CallBack

	setmetatable(self, DraggableObject)

