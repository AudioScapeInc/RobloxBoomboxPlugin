			local SearchBarText = Handler_Interface:Get("SearchBarText") :: string
			local GenreBoxSelected = Handler_Interface:Get("GenreBoxSelected") :: string

			if Searching then
				return false
			end

			if SearchTab ~= "Library" then
				return true
			end

			if #SearchBarText > 0 or GenreBoxSelected then
				return true
			end

