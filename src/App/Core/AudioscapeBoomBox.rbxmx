				local Queue = {}
				for i = 1, math.clamp(#Results, 2, 40) do
					table.insert(Queue, Results[i])
				end

				Handlers_Interface:Fire("PlaySound", Results[1])

				Handlers_Interface:Set("Queue", Queue)
			end
		end
	else
		Events.Audioscape:InvokeServer(Action, Payload)
	end
end)

