local function Request(Player: Player, Action: string, Payload: string | { [any]: any })
	local SecretIsString = type(API_KEY) == "string"

	local url = SecretIsString and "https://0ibi1y5igg.execute-api.us-east-2.amazonaws.com/dev/"
