		if not CurrentData then
			Persistence:SetAsync(Player.UserId, HttpService:JSONEncode(TemplateData))
			return TemplateData
		end

		return CurrentData
	elseif RequestType == "POST" then
		Persistence:SetAsync(Player.UserId, HttpService:JSONEncode(Payload))

		return true
	end

	return false
end

