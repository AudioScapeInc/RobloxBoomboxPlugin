		return
	end

	local Queue = Handlers_Interface:Get("Queue") :: { [any]: any }
	local QueueLength = #Queue
	local NewIndex = math.clamp(Index, 1, QueueLength)

	--// Play First Song
	Handlers_Interface:Set("Sound", Queue[NewIndex])
